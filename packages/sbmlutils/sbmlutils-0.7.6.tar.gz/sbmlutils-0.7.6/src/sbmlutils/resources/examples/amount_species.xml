    <listOfUnitDefinitions>
      <unitDefinition id="l_per_ml" name="l/ml">
        <listOfUnits>
          <unit kind="litre" exponent="1" scale="0" multiplier="1"/>
          <unit kind="litre" exponent="-1" scale="0" multiplier="0.001"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m2" name="meter^2">
        <listOfUnits>
          <unit kind="metre" exponent="2" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m3" name="meter^3">
        <listOfUnits>
          <unit kind="metre" exponent="3" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="ml_per_l" name="ml/l">
        <listOfUnits>
          <unit kind="litre" exponent="1" scale="0" multiplier="0.001"/>
          <unit kind="litre" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mmole" name="mmole">
        <listOfUnits>
          <unit kind="mole" exponent="1" scale="0" multiplier="0.001"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mmole_per_s" name="mmole/s">
        <listOfUnits>
          <unit kind="mole" exponent="1" scale="0" multiplier="0.001"/>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="per_s" name="1/s">
        <listOfUnits>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfCompartments>
      <compartment metaid="meta_Vc" sboTerm="SBO:0000290" id="Vc" name="cell compartment" spatialDimensions="3" size="1e-06" units="m3" constant="false">
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_Vc">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000290"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
      </compartment>
    </listOfCompartments>
    <listOfSpecies>
      <species metaid="meta_Aglc" sboTerm="SBO:0000247" id="Aglc" name="glucose" compartment="Vc" initialAmount="5" substanceUnits="mmole" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml" style="color: #333;line-height: 1.6;padding-bottom: 10px;padding: 30px;font-family: Helvetica, arial, sans-serif;font-size: 14px;background-color: white;padding-top: 10px;">
            <p style="margin-top: 0 !important;margin-bottom: 0 !important;margin: 15px 0;">Species in amounts. The unit of the species is the <code style="border-radius: 3px;background-color: #f8f8f8;white-space: nowrap;margin: 0 2px;padding: 0 5px;border: 1px solid #eaeaea;">substanceUnit</code>,
i.e. [mmole].</p>
          </body>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_Aglc">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000247"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
      </species>
      <species metaid="meta_Cglc6p" sboTerm="SBO:0000247" id="Cglc6p" name="glucose 6-phosphate" compartment="Vc" initialAmount="0" substanceUnits="mmole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml" style="font-size: 14px;line-height: 1.6;padding-top: 10px;font-family: Helvetica, arial, sans-serif;padding-bottom: 10px;background-color: white;color: #333;padding: 30px;">
            <p style="margin-top: 0 !important;margin: 15px 0;margin-bottom: 0 !important;">Species in concentration via the <code style="background-color: #f8f8f8;padding: 0 5px;margin: 0 2px;border: 1px solid #eaeaea;white-space: nowrap;border-radius: 3px;">hasOnlySubstanceUnit=False</code>.
The unit of the species is the
<code style="border-radius: 3px;border: 1px solid #eaeaea;padding: 0 5px;white-space: nowrap;margin: 0 2px;background-color: #f8f8f8;">substanceUnit/compartmentUnit</code>, i.e. [mmole/litre].</p>
          </body>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_Cglc6p">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000247"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
      </species>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="conversion_ml_per_l" name="volume conversion factor" value="1000" units="ml_per_l" constant="true"/>
      <parameter id="conversion_l_per_ml" name="volume conversion factor" value="0.001" units="l_per_ml" constant="true"/>
      <parameter id="k1" value="1" units="per_s" constant="true"/>
    </listOfParameters>
    <listOfRules>
      <assignmentRule variable="Vc">
        <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
          <apply>
            <times/>
            <cn sbml:units="m3"> 2 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                <cn sbml:units="second" type="integer"> 1 </cn>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
    <listOfReactions>
      <reaction metaid="meta_R1" sboTerm="SBO:0000176" id="R1" reversible="false" fast="false" compartment="Vc">
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_R1">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000176"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
        <listOfReactants>
          <speciesReference species="Aglc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Cglc6p" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> k1 </ci>
              <ci> Aglc </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
