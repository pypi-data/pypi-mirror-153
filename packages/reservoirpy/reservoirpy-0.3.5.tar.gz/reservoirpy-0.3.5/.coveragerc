# Configuration for coverage.py

[run]
branch = True
source = reservoirpy
include = */reservoirpy/*
omit =
    */setup.py
    */tests/*
    */examples/*
    */experimental/*
    */compat/*
    */hyper/*
    */type.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if self.verbose:
    if verbose:
show_missing = True
