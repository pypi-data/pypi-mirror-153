{
  "description": "Asynchronous database connection management",
  "author": "Cochise Ruhulessin",
  "author_email": "cochise.ruhulessin@unimatrixone.io",
  "url": "https://gitlab.com/unimatrixone",
  "licenses": "Proprietary",
  "entry_points": {
    "pytest11": [
      "unimatrix-rdbms = unimatrix.ext.rdbms.__pytest__"
    ],
    "unimatrix.runtime.init": [
      "unimatrix.ext.rdbms = unimatrix.ext.rdbms:connect"
    ],
    "unimatrix.runtime.cleanup": [
      "unimatrix.ext.rdbms = unimatrix.ext.rdbms:disconnect"
    ]
  },
  "extras_require": {
    "all": [
      "asyncpg",
      "aiomysql",
      "aiosqlite",
      "psycopg2-binary",
      "pytest-asyncio"
    ]
  },
  "classifiers": [
     "Development Status :: 4 - Beta",
     "Environment :: Console",
     "Environment :: Web Environment",
     "Intended Audience :: Developers",
     "License :: Other/Proprietary License",
     "Operating System :: POSIX",
     "Programming Language :: Python :: 3.8",
     "Programming Language :: Python :: 3.9",
     "Programming Language :: Python :: 3.10",
     "Programming Language :: Python :: 3 :: Only",
     "Topic :: Software Development :: Libraries"
  ]
}
