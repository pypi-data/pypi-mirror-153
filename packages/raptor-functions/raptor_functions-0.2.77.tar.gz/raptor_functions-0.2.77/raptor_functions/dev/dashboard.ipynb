    "# load dataset\n",
    "from pycaret.datasets import get_data\n",
    "juice = get_data('juice')\n",
    "\n",
    "# init setup\n",
    "from pycaret.classification import *\n",
    "exp_name = setup(data = juice,  target = 'Purchase', silent=True, verbose=False) \n",
    "\n",
    "# train model\n",
    "lr = create_model('lr')\n",
    "\n",
    "# launch dashboard\n",
    "dashboard(lr)"
